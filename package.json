{
  "name": "novoCD",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:cdptbr": "turbo build --filter=cdptbr",
    "dev": "turbo run dev",
    "dev:cdptbr": "turbo dev --filter=cdptbr",
    "dev:web": "turbo dev --filter=web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
